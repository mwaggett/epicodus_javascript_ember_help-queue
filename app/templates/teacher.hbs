{{#each model as |ticket|}}

    <div class="ticket">

      <h4 class="pull-right inline">{{#link-to 'teacher-ticket' ticket.id}}>{{/link-to}}</h4>
      <h3 class="inline">{{ticket.name}}</h3>
      <h6>{{time-since-submitted ticket.when_added}}</h6>
    </div>

{{/each}}

